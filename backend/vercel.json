{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/auth/(.*)", "dest": "/api/auth/$1" },
    { "src": "/health", "dest": "/api/health" },
    { "src": "/", "dest": "/api" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/api/$1" }
  ]
}